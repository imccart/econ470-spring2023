	keywords = {wages},
	pages = {251--333},
	file = {Abowd Kramarz and Margolis 1999.pdf:/home/immccar/Zotero/storage/5JWL77ZG/Abowd Kramarz and Margolis 1999.pdf:application/pdf;Full Text PDF:/home/immccar/Zotero/storage/A2A2MJDL/Abowd et al. - 1999 - High Wage Workers and High Wage Firms.pdf:application/pdf;Snapshot:/home/immccar/Zotero/storage/TRZDSHRR/1468-0262.html:text/html;Snapshot:/home/immccar/Zotero/storage/K52YGINY/1468-0262.html:text/html},
