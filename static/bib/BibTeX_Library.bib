	author = {Richards, Michael R. and Seward, Jonathan A. and Whaley, Christopher M.},
	month = mar,
	year = {2021},
	keywords = {Aged, Arthroplasty, Replacement, Knee, Humans, Inpatients, Medicare, Outpatients, Retrospective Studies, United States},
	pages = {104--108},
}

