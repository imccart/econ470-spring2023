	keywords = {clustering},
	file = {Abadie et al 2017.pdf:/home/immccar/Zotero/storage/IS7GWA3F/Abadie et al 2017.pdf:application/pdf;Full Text PDF:/home/immccar/Zotero/storage/JLLU5TLA/Abadie et al. - 2017 - When Should You Adjust Standard Errors for Cluster.pdf:application/pdf;Full Text PDF:/home/immccar/Zotero/storage/MAP2T7RF/Abadie et al. - 2017 - When Should You Adjust Standard Errors for Cluster.pdf:application/pdf},
